export OFFLOAD_INIT=on_offload
export MPIRETURN=999
export LD_LIBRARY_PATH="/gscratch/home/stetef/mpich-3.2/lib"
python [PIPELINE_SCRIPT=] [COMPOUND_NAME=] [WORK_DIR=] [OUT_DIR=] [CORES=28]
