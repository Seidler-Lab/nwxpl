#!/bin/csh -f
#SBATCH --job-name=[JOB_NAME=]
#SBATCH --account=stf
#SBATCH --partition=stf
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=[CORES=28]
#SBATCH --time=[TIME=10:00:00]
#SBATCH --mem=[MEMORY=50G]
#SBATCH --output=[WORK_DIR=]/slurm-cid-[COMPOUND_NAME=].out
## Adding email
#SBATCH --mail-type=END
#SBATCH --mail-user=stetef@uw.edu
module purge
module load contrib/python_3.8/3.8
module load contrib/NWChem
export ARMCI_DEFAULT_SHMMAX=32768
export NWCHEM_BASIS_LIBRARY="/sw/contrib/NWChem/nwchem-6.6/src/basis/libraries/"
